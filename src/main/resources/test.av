object1 := {
	id 1
	name "Object One"
	color red
	size large
	features [feature1 feature2]
}

object2 := {
	id 2
	name "Object Two"
	color blue
	shape circle
	features [feature3 feature4]
}

arr1 := [1 2 3 4 5]
arr2 := [3 4 5 4 3]

map1 := {1: 2 3: 4 5: 6}
map2 := {3: 5 5: 4 7: 2}

object_overlay 'object1 ~ 'object2
object_unite 'object1 + 'object2
object_default 'object1 | 'object2
object_intersect 'object1 & 'object2
object_differ 'object1 - 'object2
object_either 'object1 ^ 'object2
object_push 'object1 @ 'object2

arr_overlay 'arr1 ~ 'arr2
arr_unite 'arr1 + 'arr2
arr_default 'arr1 | 'arr2
arr_intersect 'arr1 & 'arr2
arr_differ 'arr1 - 'arr2
arr_either 'arr1 ^ 'arr2
arr_push 'arr1 @ 'arr2

map_overlay 'map1 ~ 'map2
map_unite 'map1 + 'map2
map_default 'map1 | 'map2
map_intersect 'map1 & 'map2
map_differ 'map1 - 'map2
map_either 'map1 ^ 'map2
map_push 'map1 @ 'map2